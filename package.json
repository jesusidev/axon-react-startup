{
  "name": "axon-react-startup",
  "version": "1.0.0",
  "description": "Axon Bridge React Startup Framework",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all dev lint -- --watch",
    "build": "npm-run-all -p lint:fix prod",
    "dev": "webpack-dev-server --env dev",
    "staging": "webpack --env staging",
    "prod": "webpack --env prod",
    "lint": "tsc && eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "tsc --noEmit && eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
    "format": "prettier --write \"./src/**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|md)\"",
    "analyze": "webpack-bundle-analyzer --port 4200 public/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdevtech/axon-react-startup.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Jesus Guzman (gdevtech)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gdevtech/axon-react-startup/issues"
  },
  "homepage": "https://github.com/gdevtech/axon-react-startup#readme",
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-autofix": "^1.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
