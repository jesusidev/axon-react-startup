{
  "name": "axon-react-startup",
  "version": "1.6.0",
  "description": "Axon Bridge React Startup Framework",
  "main": "index.tsx",
  "scripts": {
    "prestart": "npm-run-all --silent -p remove:coverage remove:public",
    "start": "npm-run-all --silent -p lint:fix test:watch dev lint:watch format:watch eslint:watch",
    "build": "npm-run-all -p test lint:fix prod",
    "test": "jest --coverage",
    "test:watch": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "dev": "webpack-dev-server --env development",
    "staging": "webpack --env staging",
    "prod": "webpack --env production",
    "lint": "tsc && eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "tsc --noEmit && eslint ./src/**/*.{ts,tsx} --fix",
    "lint:watch": "tsc --watch --pretty",
    "eslint:watch": "esw ./src/ -w --clear --changed",
    "format": "prettier --write \"./src/**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|md)\"",
    "format:watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "analyze": "webpack-bundle-analyzer --port 4200 public/stats.json",
    "remove:coverage": "rimraf ./coverage",
    "remove:public": "rimraf ./public",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdevtech/axon-react-startup.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Jesus Guzman (gdevtech)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gdevtech/axon-react-startup/issues"
  },
  "homepage": "https://github.com/gdevtech/axon-react-startup#readme",
  "dependencies": {
    "@types/styled-components": "^5.1.0",
    "babel-jest": "^25.3.0",
    "focus-visible": "^5.1.0",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "snyk": "^1.448.0"
  },
  "devDependencies": {
    "@architect/sandbox": "^1.9.2",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-autofix": "^1.0.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-notifier-webpack-plugin": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.3.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-styled-components": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "svg-url-loader": "^5.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "typescript-plugin-styled-components": "^1.4.4",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "snyk": true
}
